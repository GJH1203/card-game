# Production Environment Variables
# Copy this to .env and fill in real values

# MongoDB Configuration (for docker-compose)
MONGO_USERNAME=admin
MONGO_PASSWORD=your_secure_password_here

# MongoDB URI for Spring Boot application
MONGODB_URI=mongodb://admin:your_secure_password_here@localhost:27017/card_game?authSource=admin

# Nakama Configuration
NAKAMA_HOST=localhost
NAKAMA_PORT=7349
NAKAMA_SERVER_KEY=your_nakama_key_here

# Spring Configuration
SPRING_PROFILES_ACTIVE=production